{
    "manifest_version": 2,
    "name": "Internet Wrapped",
    "version": "2.2",
    "description": "Generate fun, summarized highlights from your browsing history in 2025!",
    "icons": {
      "48": "icons/ic48.png",
      "96": "icons/ic96.png"
    },
    "permissions": [
        "history"
    ],
    "browser_action": {
        "default_icon": {
             "48": "icons/ic48.png",
            "96": "icons/ic96.png"
        }
    },
    "background": {
        "scripts": ["background.js"]
    },
    "web_accessible_resources": [
          "icons/ic128.png"
    ],
    "browser_specific_settings": {
        "gecko": {
            "id": "firefoxextension@internetwrapped.com",
            "strict_min_version": "79.0"
        }
    }
  }