{
    "manifest_version": 3,
    "name": "Internet Wrapped",
    "version": "2.0",
    "description": "Generate fun, summarized highlights from your browsing history in 2025!",
    "icons": {
        "16": "icons/ic16.png",
        "32": "icons/ic32.png",
        "48": "icons/ic48.png",
        "128": "icons/ic128.png"
    },
    "permissions": [
      "history"
    ],
    "action": {
    "default_icon": {
      "16": "icons/ic16.png",
      "32": "icons/ic32.png",
      "48": "icons/ic48.png",
      "128": "icons/ic128.png"
    }
    },
    "background": {
      "service_worker": "background.js"
    },
    "web_accessible_resources": [{
        "resources": ["icons/ic128.png"],
        "matches": ["<all_urls>"]
    }]
  }